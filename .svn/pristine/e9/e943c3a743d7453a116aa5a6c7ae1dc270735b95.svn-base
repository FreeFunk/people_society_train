<script>
        layui.config({
            base: '/layuiadmin/' //静态资源所在路径
        }).extend({
            index: 'lib/index' //主入口模块
        }).use(['index', 'form','laydate','upload'], function(){
            var $ = layui.$
                ,laydate = layui.laydate
                ,admin  = layui.admin
                ,upload = layui.upload
                ,form = layui.form;

            //监听提交
            /*form.on('submit(SocietySchool-form-submit)', function(data){
