<script src="/layuiadmin/layui/layui.js"></script>
<script>
        layui.config({
            base: '/layuiadmin/' //静态资源所在路径
        }).extend({
            index:'lib/index',
            common:'common',
            $tool:'tool'
        }).use(['index','table', 'form','common','laytpl','$tool'], function(exports){
            var $ = layui.$
                ,table = layui.table
                ,common = layui.common
                ,admin = layui.admin
                ,laytpl = layui.laytpl
                ,$tool = layui.$tool
                ,form = layui.form;



            selectMajorList();
            function selectMajorList(){
                admin.req({
                    url:layui.setter.basePath + "/society/societySchoolMajor/listBySchoolId.jsn",
                    data:{},
                    method:'post',
                    success:function(data){
                        if(data.success){
                            var getTpl = document.getElementById("MajorOptionList").innerHTML,
                                view = document.getElementById("ownerMajorId");
                            laytpl(getTpl).render(data,function(html){
                                view.innerHTML = html;
                                form.render();
                            })
                        }
                    }
                })
            }

            //查询
            form.on('submit(SocietySchoolClass-search)', function(data){
                var field = data.field;

                //执行重载
                table.reload('SocietySchoolClass-list', {
                    where: field,page:{curr:1}
                });
            });

            /*
            * 定义表格
            * */
            defineTable = function () {
                var headers = layui.admin.getTokenHeader();
                table.render({
                    elem: '#SocietySchoolClass-list' //指定原始表格的选择器（id选择器）
                    ,url:layui.setter.basePath +'/society/societySchoolClass/schoolListpage.jsn' //模拟接口
                    ,headers:headers
                    ,method: 'post'
                    ,parseData: function(res){ //res 即为原始返回的数据
                        return {
                            "code": res.success, //解析接口状态
                            "msg": res.errMsg, //解析提示文本
                            "count": res.totalCount, //解析数据长度
                            "data": res.list //解析数据列表
                        };
                    }
                    ,response: {
                        statusCode: true //规定成功的状态码，默认：0
                    }
                    ,cols: [[
                        {type: 'numbers', fixed: 'left',title: '序号'}
            ,{field: 'id',align: 'center', width: 200, title: '主键',sort: true,hide:true}
            /*,{field: 'createTime',align: 'center', width: 120, title: '创建时间'}*/
            ,{field: 'className',align: 'center', width:'15%', title: '班级名',sort: true}
            ,{field: 'classCode',align: 'center', width:'10%', title: '班级编码',sort: true}
            ,{field: 'ownerMajorName',align: 'center', width: '10%', title: '专业',sort: false}
            ,{field: 'courseName',align: 'center', width: '10%', title: '课程',sort: false}
            ,{field: 'classStartTime',align: 'center', width: '9%', title: '培训开始时间',templet: function(d){
            return common.DateFormat(d.classStartTime,"yyyy-MM-dd");},sort: true}
            ,{field: 'classEndTime',align: 'center', width: '9%', title: '培训结束时间',templet: function(d){
            return common.DateFormat(d.classEndTime,"yyyy-MM-dd");},sort: true}
            ,{field: 'classPersonNum',align: 'center', width: '7%', title: '培训人数'}
            ,{field: 'classCourseNum',align: 'center', width: '7%', title: '课时限制'}
            ,{field: 'personInCharge',align: 'center', width: '7%', title: '班主任',sort: false}
            ,{field: 'personInChargePhoneNum',align: 'center', width: '7%', title: '班主任电话',sort: false}
            ,{field: 'classStudyState',align: 'center', width: '7%', title: '班级状态',sort: false,
                templet: function(d){
                    if(d.classStudyState=='1'){
                        return "停课";
                    }else {
                        return "开课";
                    }
                }
            }
            /*,{field: 'examineState',align: 'center', width: '7%', title: '申请状态',sort: false,
                templet: function(d){
                            if(d.examineState=='0'){
                                return "审核中";
                            }else if(d.examineState=='1'){
                                return "已通过";
                            }else if(d.examineState=='-1'){
                                return "未通过";
                            }else {
                               return "未申请";
                            }
                            }
            }*/


                        /*,{field: 'finishedPersonNum',align: 'center', width: 100, title: '已完成数',sort: true}
                        ,{field: 'notFinishedPersonNum',align: 'center', width: 100, title: '未完成数',sort: true}*/
                    ,{title: '操作', minWidth: 280 ,align: 'center', fixed: 'right', toolbar: '#table-SocietySchoolClass-list'}
            ]]
            ,autoSort: false
                ,page: true
                ,limit: 50
                ,limits: [10, 20, 30,40,50]
                ,text: {
                none: '暂无相关数据'
                }
            });

                //为toolbar添加事件响应
                table.on('tool(SocietySchoolClass-list)', function(obj){ //注：tool是工具条事件名，test是table原始容器的属性 lay-filter="对应的值"
                    /*var data = obj.data;*///获得当前行数据
                    //区分事件
                    switch(obj.event){
                        case 'del':
                            deleteVo(obj);
                            break;
                        case 'edit':
                            editVo(obj);
                            break;
                        case 'courseList':
                            courseList(obj);
                            break;
                        case 'studentList':
                            studentList(obj);
                            break;
                        case 'addStudent':
                            addStudent(obj);
                            break;
                        case 'addCourse':
                            addCourse(obj);
                            break;
                        case 'callName':
                            callName(obj);
                            break;
                        case 'openClassExamine':
                            openClassExamine(obj);
                            break;
                        case 'classStateChange':
                            classStateChange(obj);
                            break;
                    };
                });

                //监听排序事件
                table.on('sort(SocietySchoolClass-list)', function(obj){ //注：sort是工具条事件名，LAY-app-province-list是table原始容器的属性 lay-filter="对应的值"
                    var sort = obj.field;
                    //默认排序
                    switch(obj.field){
                        case 'id':
                            sort ="ID";
                            break;
                        case 'createTime':
                            sort ="CREATE_TIME";
                            break;
                        case 'classCode':
                            sort ="CREATE_TIME";
                            break;
                        case 'createUserId':
                            sort ="CREATE_USER_ID";
                            break;
                        case 'createUserName':
                            sort ="CREATE_USER_NAME";
                            break;
                        case 'ownerSchoolId':
                            sort ="OWNER_SCHOOL_ID";
                            break;
                        case 'ownerSchoolName':
                            sort ="OWNER_SCHOOL_NAME";
                            break;
                        case 'ownerMajorId':
                            sort ="OWNER_MAJOR_ID";
                            break;
                        case 'ownerMajorName':
                            sort ="OWNER_MAJOR_NAME";
                            break;
                        case 'personInCharge':
                            sort ="PERSON_IN_CHARGE";
                            break;
                        case 'personInChargePhoneNum':
                            sort ="PERSON_IN_CHARGE_PHONE_NUM";
                            break;
                        case 'className':
                            sort ="CREATE_TIME";
                            break;
                        case 'classStartTime':
                            sort ="CLASS_START_TIME";
                            break;
                        case 'classEndTime':
                            sort ="CLASS_END_TIME";
                            break;
                        case 'classPersonNum':
                            sort ="CLASS_PERSON_NUM";
                            break;
                        case 'totalLessons':
                            sort ="TOTAL_LESSONS";
                            break;
                        case 'finishedPersonNum':
                            sort ="FINISHED_PERSON_NUM";
                            break;
                        case 'notFinishedPersonNum':
                            sort ="NOT_FINISHED_PERSON_NUM";
                            break;
                        case 'dataState':
                            sort ="DATA_STATE";
                            break;
                    };
                    if(obj.type==null){
                        //有些时候，你可能需要根据当前排序的字段，重新向服务端发送请求，从而实现服务端排序，如：
                        table.reload('SocietySchoolClass-list', {
                            initSort: obj//记录初始排序，如果不设的话，将无法标记表头的排序状态。
                            ,where: { //请求参数（注意：这里面的参数可任意定义，并非下面固定的格式）
                                orderBy: "CREATE_TIME ASC"//排序字段
                            }
                        });
                    }else {
                        //有些时候，你可能需要根据当前排序的字段，重新向服务端发送请求，从而实现服务端排序，如：
                        table.reload('SocietySchoolClass-list', {
                            initSort: obj//记录初始排序，如果不设的话，将无法标记表头的排序状态。
                            ,where: { //请求参数（注意：这里面的参数可任意定义，并非下面固定的格式）
                                orderBy: sort+" "+obj.type //排序字段
                            }
                        });
                    }
                });
            }
            defineTable();


            classStateChange = function(obj){
                var classState = '';
                if (obj.data.classStudyState=='1'){
                    //停课 变成 开课
                    classState = '0';
                }else {
                    //开课 变成 停课
                    classState = '1';
                }
                admin.req({
                    url:layui.setter.basePath +'/society/societySchoolClass/classStateChange.jsn',
                    data:{
                        'id':obj.data.id,
                        'classState':classState
                    },
                    method:"post",
                    success:function (data) {
                        if(data.success){
                            table.reload('SocietySchoolClass-list',
                                {page:
                                        { curr: common.deleteJumpPage(obj) // 执行页面跳转方法
                                        }
                                }
                            );
                            // layer.close(index);
                            if(classState=='1'){
                                layer.msg('停课成功', {
                                    icon: 1
                                });
                            }else{
                                layer.msg('开课成功', {
                                    icon: 1
                                });
                            }
                        }
                    },
                });
            }

            //删除
            deleteVo = function (obj) {
                if(obj.data.examineState=='1'){//通过不能修改
                    layer.msg('开班审核已经通过,不能删除班级信息', {
                        icon: 1
                    });
                }else {
                    layer.confirm('确定删除此记录吗？', function(index){
                        admin.req({
                            url:layui.setter.basePath +'/society/societySchoolClass/logicDeleteByIds.jsn',
                            data:{'ids':obj.data.id},
                            method:"post",
                            success:function (data) {
                                if(data.success){
                                    table.reload('SocietySchoolClass-list',
                                        {page:
                                                { curr: common.deleteJumpPage(obj) // 执行页面跳转方法
                                                }
                                        }
                                    );
                                    layer.close(index);
                                    layer.msg('删除成功', {
                                        icon: 1
                                    });
                                }
                            },
                        });
                    });
                }

            }

            //开班申请
            openClassExamine = function(obj){
                if(obj.data.examineState=='1'){//通过不能修改
                    layer.msg('开班审核已经通过,不能重复提交申请', {
                        icon: 1
                    });
                }else {
                    layer.confirm('确定提交开班申请吗？', function(index){
                        admin.req({
                            url:layui.setter.basePath +'/society/societySchoolClass/updateExamineState.jsn',
                            data:{'classId':obj.data.id},
                            method:"post",
                            success:function (data) {
                                if(data.success){
                                    layer.close(index);
                                    table.reload('SocietySchoolClass-list');
                                    layer.msg('开班申请提交成功', {
                                        icon: 1
                                    });
                                }
                            }
                        });
                    });
                }
            }


            //编辑
            editVo = function (obj) {
                //判断是否审核通过
                if(obj.data.examineState=='1'){//通过不能修改
                    layer.open({
                        type: 2
                        ,title: '编辑'
                        ,skin: 'demo-class'
                        ,content: 'SocietySchoolClassEditSecond.html'
                        ,area: ['550px', '350px']
                        ,btn: ['添加学员','保存','取消']
                        ,success: function(layero, index){
                            var s = layero.find('iframe');
                            $("id",s[0].contentWindow.document).val(obj.data.id);
                            //给iframe元素赋值
                            var othis = layero.find('iframe').contents().find("#SocietySchoolClassEditSecond-form").click();

                            othis.find('input[name="id"]').val(obj.data.id);
                            othis.find('input[name="createTime"]').val(obj.data.createTime);
                            othis.find('input[name="createUserId"]').val(obj.data.createUserId);
                            othis.find('input[name="createUserName"]').val(obj.data.createUserName);
                            othis.find('input[name="ownerSchoolId"]').val(obj.data.ownerSchoolId);
                            othis.find('input[name="ownerSchoolName"]').val(obj.data.ownerSchoolName);
                            othis.find('input[id="ownerMajorId"]').val(obj.data.ownerMajorId);
                            othis.find('input[name="ownerMajorName"]').val(obj.data.ownerMajorName);
                            othis.find('input[name="personInCharge"]').val(obj.data.personInCharge);
                            othis.find('input[name="personInChargePhoneNum"]').val(obj.data.personInChargePhoneNum);
                            othis.find('input[name="className"]').val(obj.data.className);
                            othis.find('input[name="classCode"]').val(obj.data.classCode);
                            othis.find('input[name="classStartTime"]').val(common.DateFormat(obj.data.classStartTime,"yyyy-MM-dd"));
                            othis.find('input[name="classEndTime"]').val(common.DateFormat(obj.data.classEndTime,"yyyy-MM-dd"));
                            othis.find('input[name="classPersonNum"]').val(obj.data.classPersonNum);
                            othis.find('input[name="totalLessons"]').val(obj.data.totalLessons);
                            othis.find('input[name="finishedPersonNum"]').val(obj.data.finishedPersonNum);
                            othis.find('input[name="notFinishedPersonNum"]').val(obj.data.notFinishedPersonNum);
                            othis.find('input[name="dataState"]').val(obj.data.dataState);
                            othis.find('input[name="classAdminInputId"]').val(obj.data.classAdminId);
                        }
                        ,yes: function(index, layero){
                            //点击确认触发 iframe 内容中的按钮提交
                            var submit = layero.find('iframe').contents().find("#SocietySchoolClass-next-edit");
                            submit.click();
                            var dataAll = $("#dataAll").val();
                            var dataAllFlag = $("#dataAllFlag").val();
                            if(dataAllFlag){
                                var str = dataAll.split(",");
                                //添加学生
                                layer.open({
                                    type: 2
                                    ,title: '添加学生'
                                    ,skin: 'demo-class-student'
                                    ,content: 'SocietySchoolClassAndStudentVoClass.html?classId='+str[0]+'&ownerMajorId='+str[1]
                                    ,area: ['1171px', '569px']
                                    ,btn: ['完成']
                                    ,success:function(layero,index){

                                    }
                                    ,yes: function(index, layero){
                                        //点击确认触发 iframe 内容中的按钮提交
                                        var submit = layero.find('iframe').contents().find("#SocietySchoolClassAndStudent-form-submit");
                                        submit.click();
                                        $("#dataAll").val('');
                                        $("#dataAllFlag").val('');
                                        $('#courseId').val('');
                                    }
                                });
                            }
                        }
                        ,btn2: function(index, layero){
                            var submit = layero.find('iframe').contents().find("#SocietySchoolClass-form-edit");
                            submit.trigger('click');
                            $("#dataAll").val('');
                            $("#dataAllFlag").val('');
                            $('#courseId').val('');
                        }
                        ,btn3: function(index, layero){
                            layer.closeAll();
                            $("#dataAll").val('');
                            $("#dataAllFlag").val('');
                            $('#courseId').val('');
                        }
                    });
                }else {

                    admin.req({
                        url:layui.setter.basePath +'/society/societySchoolClassAndStudent/selectVoClassId.jsn',
                        data:{'classId':obj.data.id},
                        method:"post",
                        success:function (data) {
                            if(data.data.numStudent==0){
                                layer.open({
                                    type: 2
                                    ,title: '编辑'
                                    ,skin: 'demo-class'
                                    ,content: 'SocietySchoolClassEdit.html'
                                    ,area: ['700px', '550px']
                                    ,btn: ['添加学员','保存','取消']
                                    ,success: function(layero, index){
                                        var s = layero.find('iframe');
                                        $("id",s[0].contentWindow.document).val(obj.data.id);
                                        //给iframe元素赋值
                                        var othis = layero.find('iframe').contents().find("#SocietySchoolClass-form").click();

                                        othis.find('input[name="id"]').val(obj.data.id);
                                        othis.find('input[id="courseInputId"]').val(data.data.courseId);
                                        othis.find('input[name="createTime"]').val(obj.data.createTime);
                                        othis.find('input[name="createUserId"]').val(obj.data.createUserId);
                                        othis.find('input[name="createUserName"]').val(obj.data.createUserName);
                                        othis.find('input[name="ownerSchoolId"]').val(obj.data.ownerSchoolId);
                                        othis.find('input[name="ownerSchoolName"]').val(obj.data.ownerSchoolName);
                                        othis.find('input[id="ownerMajorId"]').val(obj.data.ownerMajorId);
                                        othis.find('input[name="ownerMajorName"]').val(obj.data.ownerMajorName);
                                        othis.find('input[name="personInCharge"]').val(obj.data.personInCharge);
                                        othis.find('input[name="personInChargePhoneNum"]').val(obj.data.personInChargePhoneNum);
                                        othis.find('input[name="className"]').val(obj.data.className);
                                        othis.find('input[name="classCode"]').val(obj.data.classCode);
                                        othis.find('input[name="classStartTime"]').val(common.DateFormat(obj.data.classStartTime,"yyyy-MM-dd"));
                                        othis.find('input[name="classEndTime"]').val(common.DateFormat(obj.data.classEndTime,"yyyy-MM-dd"));
                                        othis.find('input[name="classPersonNum"]').val(obj.data.classPersonNum);
                                        othis.find('input[name="totalLessons"]').val(obj.data.totalLessons);
                                        othis.find('input[name="finishedPersonNum"]').val(obj.data.finishedPersonNum);
