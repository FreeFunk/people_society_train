package com.edgedo.society.exception;

import com.edgedo.common.base.BusinessException;

/**
 * @author WangZhen
 * @description
 * @date 2020/5/4
 */
public class StudentNotRealnameException  extends BusinessException {

    public StudentNotRealnameException() {
        super();
        setErrCode("notRealName");
    }

    public StudentNotRealnameException(String errmsg) {
        super(errmsg);
        setErrCode("notRealName");
    }

    public StudentNotRealnameException(String message, Throwable cause) {
        super(message, cause);
        setErrCode("notRealName");
    }

    public StudentNotRealnameException(Throwable cause) {
        super(cause);
        setErrCode("notRealName");
    }
}
