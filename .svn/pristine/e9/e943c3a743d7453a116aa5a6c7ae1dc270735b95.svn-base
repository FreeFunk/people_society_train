                //提交 Ajax 成功后，关闭当前弹层并重载表格
                admin.req({
                    url:layui.setter.basePath +'/society/societySchool/saveOrUpdate.jsn',
                    data:field,
                    method:"post",
                    success:function (data) {
                        if(data.success){
                            parent.layui.table.reload('SocietySchool-list'); //重载表格
                            parent.layer.close(index); //再执行关闭
                            parent.layer.msg('添加成功', {
                                icon: 1
                            });
                        }else{
                            parent.layer.msg(data.errMsg, {
                            });
                        }

                    },
                });

            });*/
            //执行实例
            var uploadInst = upload.render({
                elem: '#schoolLogoUrl' //绑定元素
                ,url: '/upload/'
                ,accept: 'images'
                ,size:50000
                ,done: function(res){
                    //上传完毕回调
                }
                ,error: function(){
                    //请求异常回调
                }
            });
            form.on('submit(SocietySchool-form-submit)', function(data){
                var field = data.field; //获取提交的字段
                var index = parent.layer.getFrameIndex(window.name); //先得到当前iframe层的索引
                //提交 Ajax 成功后，关闭当前弹层并重载表格
                admin.req({
                    url:layui.setter.basePath +'/society/societySchool/addNewSchoolInfo.jsn',
                    data:field,
                    method:"post",
                    success:function (data) {
                        if(data.success){
                            parent.layui.table.reload('SocietySchool-list'); //重载表格
                            parent.layer.close(index); //再执行关闭
                            parent.layer.msg('添加成功', {
                                icon: 1
                            });
                        }else{
                            parent.layer.msg(data.errMsg, {
                            });
                        }

                    },
                });

            });

            //查询所有地区
            function addressList() {
                admin.req({
                    url:layui.setter.basePath + '/society/societySchool/listAllXianQu.jsn',
                    method:"post",
                    success:function(data){
                        debugger
                        var xianQu = data.data;
                        //将查询到的角色列表展示到下拉框中去;
                        if(xianQu!="undefined"){
                            for(var i=0;i<xianQu.length;i++){
                                var xianQuInfo = xianQu[i].id+","+xianQu[i].name;//id拼接名字
                                $("#xianquName").append("<option value="+xianQuInfo+">"+xianQu[i].name+"</option>");
                                layui.form.render('select');//重新渲染表格
                            }
                        }
                    }
                });
            }
            addressList();

            laydate.render({
                elem: '#createTime', //指定元素
                type:'datetime'
            });
        })
</script>
